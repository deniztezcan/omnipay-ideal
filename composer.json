{
    "name": "futurum/omnipay-ideal",
    "type": "library",
    "description": "iDeal driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "iDeal",
        "ideal"
    ],
    "version": "1.6",
    "homepage": "https://github.com/deniztezcan/omnipay-ideal",
    "license": "MIT",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "email": "deniz@tezcanit.solutions"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\iDeal\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\iDeal\\Tests\\": "tests/" }
    },
    "require": {
        "league/omnipay": "^3.2",
        "spatie/array-to-xml": "^3",
        "nesbot/carbon": "2.*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "omnipay/tests": "^4"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
